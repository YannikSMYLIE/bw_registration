plugin.tx_bwregistration {
    view {
        templateRootPaths {
            0 = EXT:bw_registration/Resources/Private/Common/Templates/
            1 = EXT:bw_registration/Resources/Private/Frontend/Templates/
            2 = {$plugin.tx_bwregistration.view.templateRootPath}
        }
        partialRootPaths {
            0 = EXT:bw_registration/Resources/Private/Common/Partials/
            1 = EXT:bw_registration/Resources/Private/Frontend/Partials/
            2 = {$plugin.tx_bwregistration.view.partialRootPath}
        }
        layoutRootPaths {
            0 = EXT:bw_registration/Resources/Private/Common/Layouts/
            1 = EXT:bw_registration/Resources/Private/Frontend/Layouts/
            2 = {$plugin.tx_bwregistration.view.layoutRootPath}
        }
    }
    persistence {
        storagePid = {$plugin.tx_bwregistration.persistence.storagePid}
    }
    settings {
        standalone {
            typeNum = {$plugin.tx_bwregistration.settings.standalone.typeNum}
        }
    }
}

module.tx_bwregistration {
    view {
        templateRootPaths {
            0 = EXT:bw_registration/Resources/Private/Common/Templates/
            1 = EXT:bw_registration/Resources/Private/Backend/Templates/
            2 = {$module.tx_bwregistration.view.templateRootPath}
        }
        partialRootPaths {
            0 = EXT:bw_registration/Resources/Private/Common/Partials/
            1 = EXT:bw_registration/Resources/Private/Backend/Partials/
            2 = {$module.tx_bwregistration.view.partialRootPath}
        }
        layoutRootPaths {
            0 = EXT:bw_registration/Resources/Private/Common/Layouts/
            1 = EXT:bw_registration/Resources/Private/Backend/Layouts/
            2 = {$module.tx_bwregistration.view.layoutRootPath}
        }
    }
    persistence {
        storagePid = {$module.tx_bwregistration.persistence.storagePid}
    }
}

page {
    includeJSFooter {
        bw_registration = EXT:bw_registration/Resources/Public/JavaScript/PersonModule.js
    }
}